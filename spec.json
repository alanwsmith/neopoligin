  "neo_sections": [
    {
      "name": "Aside: Just Text",
      "notes": [],
      "parts": {
        "input": "-- aside\n\nalfa bravo\n\ncharlie\ndelta",
        "expected": [
          {
            "type": "aside",
            "attrs": null,
            "blocks": [
              {
                "type": "paragraph",
                "snippets": [
                  {
                    "type": "text",
                    "text": "alfa bravo"
                  }
                ]
              },
              {
                "type": "paragraph",
                "snippets": [
                  {
                    "type": "text",
                    "text": "charlie delta"
                  }
                ]
              }
            ]
          }
        ]
      }
    },

