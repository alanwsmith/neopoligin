{
    "tests": [
      {
        "title": "endarticle on one line",
        "input": "-- endarticle\n\nfoxtrot delta echo\n",
        "expected": [
          {
            "attributes": null,
            "type": "endarticle",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "foxtrot" },
                  { "type": "space" },
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "echo" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "endarticle attributes",
        "input": "-- endarticle\n-- id: tango\n\nfoxtrot delta echo\n",
        "expected": [
          {
            "attributes": [{ "type": "id", "value": "tango" }],
            "type": "endarticle",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "foxtrot" },
                  { "type": "space" },
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "echo" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "endarticle on one line without line ending",
        "input": "-- endarticle\n\ndelta sierra",
        "expected": [
          {
            "attributes": null,
            "type": "endarticle",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "sierra" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "endarticle split across lines",
        "input": "-- endarticle\n\nbravo delta\nsierra charlie\n",
        "expected": [
          {
            "attributes": null,
            "type": "endarticle",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "bravo" },
                  { "type": "space" },
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "sierra" },
                  { "type": "space" },
                  { "type": "text", "string": "charlie" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "endarticle multi paragraph",
        "input": "-- endarticle\n\ntango whiskey\nvictor mike\n\nhotel sierra\n",
        "expected": [
          {
            "attributes": null,
            "type": "endarticle",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "tango" },
                  { "type": "space" },
                  { "type": "text", "string": "whiskey" },
                  { "type": "space" },
                  { "type": "text", "string": "victor" },
                  { "type": "space" },
                  { "type": "text", "string": "mike" }
                ]
              },
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "hotel" },
                  { "type": "space" },
                  { "type": "text", "string": "sierra" }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
  