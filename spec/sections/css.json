{
    "TODOs": [],
    "tests": [
      {
        "title": "css basic",
        "input": "-- css\n\nbody { background-color: #123; }\n\n-- ",
        "expected": [
          {
            "attributes": null,
            "type": "css",
            "body": "body { background-color: #123; }"
          }
        ]
      },
      {
        "title": "css show",
        "input": "-- css\n-- show\n\nbody { background-color: #aabbcc; }\n\n-- ",
        "expected": [
          {
            "attributes": [{"type": "show"}],
            "type": "css",
            "body": "body { background-color: #aabbcc; }"
          }
        ]
      },
      {
        "title": "css",
        "input": "-- css\n-- hidden\n\nbody { background-color: #aabbcc; }\n\n-- ",
        "expected": [
          {
            "attributes": [{"type": "hidden"}],
            "type": "css",
            "body": "body { background-color: #aabbcc; }"
          }
        ]
      }
    ]
  }
  
