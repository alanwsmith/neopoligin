      "expected": [
        {
          "attributes": null,
          "type": "css",
          "body": "body { background-color: #123; }"
        },
        {
          "attributes": null,
          "type": "p",
          "body": [
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "alfa" },
                { "type": "space" },
                { "type": "text", "string": "kilo" }
              ]
            }
          ]
        }
      ]
    }

  ]
