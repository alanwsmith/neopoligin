{
  "tests": [
    {
      "notes": ["Used for attribution of quotes and blockquotes"],
      "title": "Attributes: by",
      "input": "-- blockquote\n-- by: alfa echo\n",
      "expected": [
        {
          "attributes": 
          [{"type": "by", "value": "alfa echo"}],
          "type": "blockquote",
          "body": null
        }
      ]
    },
    {
      "title": "Attributes: source",
      "input": "-- blockquote\n-- source: kilo charlie\n",
      "expected": [
        {
          "attributes": 
          [{"type": "source", "value": "kilo charlie"}],
          "type": "blockquote",
          "body": null
        }
      ]
    },
    {
      "title": "Attributes: title",
      "input": "-- p\n-- title: sierra romeo\n",
      "expected": [
        {
          "attributes": 
          [{"type": "title", "value": "sierra romeo"}],
          "type": "p",
          "body": null
        }
      ]
    },
    {
      "title": "Attributes: hidden",
      "input": "-- p\n-- hidden\n",
      "expected": [
        {
          "attributes": 
          [{"type": "hidden"}],
          "type": "p",
          "body": null
        }
      ]
    },
    {
      "title": "Attributes: show (default value: below)",
      "input": "-- p\n-- show\n",
      "expected": [
        {
          "attributes": 
          [{"type": "show", "value": "below"}],
          "type": "p",
          "body": null
        }
      ]
    },
    {
      "title": "Attributes: show (specified value)",
      "input": "-- p\n-- show: above\n",
      "expected": [
        {
          "attributes": 
          [{"type": "show", "value": "above"}],
          "type": "p",
          "body": null
        }
      ]
    },
    {
      "title": "Attributes: showtitle",
      "input": "-- p\n-- showtitle: juliette echo\n",
      "expected": [
        {
          "attributes": 
          [{"type": "showtitle", "value": "juliette echo"}],
          "type": "p",
          "body": null
        }
      ]
    },
    {
      "title": "Attributes: url",
      "input": "-- p\n-- url: https://www.example.com/\n",
      "expected": [
        {
          "attributes": 
          [{"type": "url", "value": "https://www.example.com/"}],
          "type": "p",
          "body": null
        }
      ]
    }
  ]
}
