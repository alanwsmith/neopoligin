{
  "tests": [
    {
      "title": "endtldr on one line",
      "input": "-- endtldr\n\nfoxtrot delta echo\n",
      "expected": [
        {
          "attributes": null,
          "type": "endtldr",
          "body": [
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "foxtrot" },
                { "type": "space" },
                { "type": "text", "string": "delta" },
                { "type": "space" },
                { "type": "text", "string": "echo" }
              ]
            }
          ]
        }
      ]
    },
    {
      "title": "endtldr attributes",
      "input": "-- endtldr\n-- id: tango\n\nfoxtrot delta echo\n",
      "expected": [
        {
          "attributes": [{ "key": "id", "value": "tango" }],
          "type": "endtldr",
          "body": [
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "foxtrot" },
                { "type": "space" },
                { "type": "text", "string": "delta" },
                { "type": "space" },
                { "type": "text", "string": "echo" }
              ]
            }
          ]
        }
      ]
    },
    {
      "title": "endtldr on one line without line ending",
      "input": "-- endtldr\n\ndelta sierra",
      "expected": [
        {
          "attributes": null,
          "type": "endtldr",
          "body": [
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "delta" },
                { "type": "space" },
                { "type": "text", "string": "sierra" }
              ]
            }
          ]
        }
      ]
    },
    {
      "title": "endtldr split across lines",
      "input": "-- endtldr\n\nbravo delta\nsierra charlie\n",
      "expected": [
        {
          "attributes": null,
          "type": "endtldr",
          "body": [
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "bravo" },
                { "type": "space" },
                { "type": "text", "string": "delta" },
                { "type": "space" },
                { "type": "text", "string": "sierra" },
                { "type": "space" },
                { "type": "text", "string": "charlie" }
              ]
            }
          ]
        }
      ]
    },
    {
      "title": "endtldr multi paragraph",
      "input": "-- endtldr\n\ntango whiskey\nvictor mike\n\nhotel sierra\n",
      "expected": [
        {
          "attributes": null,
          "type": "endtldr",
          "body": [
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "tango" },
                { "type": "space" },
                { "type": "text", "string": "whiskey" },
                { "type": "space" },
                { "type": "text", "string": "victor" },
                { "type": "space" },
                { "type": "text", "string": "mike" }
              ]
            },
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "hotel" },
                { "type": "space" },
                { "type": "text", "string": "sierra" }
              ]
            }
          ]
        }
      ]
    }
  ]
}
