{
  "TODOs": ["attributes"],
  "tests": [
    {
      "title": "startsection",
      "input": "-- startsection\n",
      "expected": [
        {
          "attributes": null,
          "body": null,
          "type": "startsection"
        }
      ]
    },
    {
      "title": "startsection with attributes",
      "input": "-- startsection\n-- class: foxtrot sierra",
      "expected": [
        {
          "attributes": [{ "type": "class", "value": ["foxtrot", "sierra"] }],
          "body": null,
          "type": "startsection"
        }
      ]
    },
    {
      "title": "startsection with paragraphs",
      "input": "-- startsection\n\njuliette kilo\n\n-- endsections",
      "expected": [
        {
          "attributes": null,
          "type": "startsection",
          "body": [
            {
              "type": "paragraph",
              "content": [
                { "type": "text", "string": "juliette" },
                { "type": "space" },
                { "type": "text", "string": "kilo" }
              ]
            }
          ]
        }
      ]
    }
  ]
}
