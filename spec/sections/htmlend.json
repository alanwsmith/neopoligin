{
    "tests": [
      {
        "title": "htmlend on one line",
        "input": "-- htmlend\n\nfoxtrot delta echo\n",
        "expected": [
          {
            "attributes": null,
            "type": "p",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "foxtrot" },
                  { "type": "space" },
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "echo" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "htmlend attributes",
        "input": "-- htmlend\n-- id: tango\n\nfoxtrot delta echo\n",
        "expected": [
          {
            "attributes": [{ "type": "id", "value": "tango" }],
            "type": "p",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "foxtrot" },
                  { "type": "space" },
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "echo" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "htmlend on one line without line ending",
        "input": "-- htmlend\n\ndelta sierra",
        "expected": [
          {
            "attributes": null,
            "type": "p",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "sierra" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "htmlend split across lines",
        "input": "-- htmlend\n\nbravo delta\nsierra charlie\n",
        "expected": [
          {
            "attributes": null,
            "type": "p",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "bravo" },
                  { "type": "space" },
                  { "type": "text", "string": "delta" },
                  { "type": "space" },
                  { "type": "text", "string": "sierra" },
                  { "type": "space" },
                  { "type": "text", "string": "charlie" }
                ]
              }
            ]
          }
        ]
      },
      {
        "title": "htmlend multi paragraph",
        "input": "-- htmlend\n\ntango whiskey\nvictor mike\n\nhotel sierra\n",
        "expected": [
          {
            "attributes": null,
            "type": "p",
            "body": [
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "tango" },
                  { "type": "space" },
                  { "type": "text", "string": "whiskey" },
                  { "type": "space" },
                  { "type": "text", "string": "victor" },
                  { "type": "space" },
                  { "type": "text", "string": "mike" }
                ]
              },
              {
                "type": "paragraph",
                "content": [
                  { "type": "text", "string": "hotel" },
                  { "type": "space" },
                  { "type": "text", "string": "sierra" }
                ]
              }
            ]
          }
        ]
      }
    ]
  }
  