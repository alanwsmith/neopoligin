  "tests": [
    {
      "title": "Startcode basic",
      "input": "-- startcode\n\n-- p\n\ndelta echo\n\n-- endcode",
      "expected": [
        {
          "attributes": null,
          "type": "code",
          "body": "-- p\n\ndelta echo"
        }
      ]
    },
    {
      "title": "Startcode with show flag",
      "input": "-- startcode\n-- show\n\nfoxtrot hotel echo\n\n-- endcode",
      "expected": [
        {
          "attributes": [{"type": "show"}],
          "type": "code",
          "body": "foxtrot hotel echo"
        }
      ]
    }
  ]
}
