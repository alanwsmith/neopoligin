{%- from "foundation/blocks/section_attrs.j2" import section_attrs -%}
{%- from "foundation/blocks/text.j2" import text -%}

{%- macro h1(content) -%}
    <h1 {{- section_attrs(content.attrs)-}}>
        {{text(content.headline.tags)}}
    </h1>
    {%- for paragraph in content.paragraphs -%}
        <p>{{- text(paragraph.tags) -}}</p>
    {%- endfor -%}
{%- endmacro -%}


