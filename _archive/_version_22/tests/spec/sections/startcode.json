{
  "tests": [
    {
      "input": "-- startcode\n\ntango foxtrot\n\n-- endcode",
      "expected": [
        {
          "attributes": null,
          "type": "code",
          "body": "tango foxtrot"
        }
      ]
    },
    {
      "input": "-- startcode\n-- id: delta\n\nsierra bravo\n\n-- endcode",
      "expected": [
        {
          "attributes": [{ "type": "id", "value": "delta" }],
          "type": "code",
          "body": "sierra bravo"
        }
      ]
    }
  ]
}
